    - me hubiera agazapado
    - me hubiese agazapado
  tu:
    - te hubieras agazapado
    - te hubieses agazapado
  vos:
    - te hubieras agazapado
    - te hubieses agazapado
  ud:
    - se hubiera agazapado
    - se hubiese agazapado
  nosotros:
    - nos hubiéramos agazapado
    - nos hubiésemos agazapado
  vosotros:
    - os hubierais agazapado
    - os hubieseis agazapado
  uds:
    - se hubieran agazapado
    - se hubiesen agazapado
imperativo:
  tu: agázapate
  vos: agáchate
  ud: agáchese
  nosotros: agazapémonos
  vosotros: agazapaos
  uds: agáchense