meaning: to alarm oneself, to get alarmed
infinitivo: alarmarse
gerundio: alarmándose
participio-pasado: alarmado

presente-indicativo:
  yo: me alarmo
  tu: te alarmas
  vos: te alarmas
  ud: se alarma
  nosotros: nos alarmamos
  vosotros: os alarmáis
  uds: se alarman

preterito-perfecto-simple:
  yo: me alarmé
  tu: te alarmaste
  vos: te alarmaste
  ud: se alarmó
  nosotros: nos alarmamos
  vosotros: os alarmasteis
  uds: se alarmaron

preterito-imperfecto:
  yo: me alarmaba
  tu: te alarmabas
  vos: te alarmabas
  ud: se alarmaba
  nosotros: nos alarmábamos
  vosotros: os alarmabais
  uds: se alarmaban

futuro-simple:
  yo: me alarmaré
  tu: te alarmarás
  vos: te alarmarás
  ud: se alarmará
  nosotros: nos alarmaremos
  vosotros: os alarmaréis
  uds: se alarmarán

condicional-simple:
  yo: me alarmaría
  tu: te alarmarías
  vos: te alarmarías
  ud: se alarmaría
  nosotros: nos alarmaríamos
  vosotros: os alarmaríais
  uds: se alarmarían

presente-perfecto:
  yo: me he alarmado
  tu: te has alarmado
  vos: te has alarmado
  ud: se ha alarmado
  nosotros: nos hemos alarmado
  vosotros: os habéis alarmado
  uds: se han alarmado

preterito-pluscuamperfecto:
  yo: me había alarmado
  tu: te habías alarmado
  vos: te habías alarmado
  ud: se había alarmado
  nosotros: nos habíamos alarmado
  vosotros: os habíais alarmado
  uds: se habían alarmado

futuro-perfecto:
  yo: me habré alarmado
  tu: te habrás alarmado
  vos: te habrás alarmado
  ud: se habrá alarmado
  nosotros: nos habremos alarmado
  vosotros: os habréis alarmado
  uds: se habrán alarmado

condicional-perfecto:
  yo: me habría alarmado
  tu: te habrías alarmado
  vos: te habrías alarmado
  ud: se habría alarmado
  nosotros: nos habríamos alarmado
  vosotros: os habríais alarmado
  uds: se habrían alarmado

presente-subjuntivo:
  yo: me alarme
  tu: te alarmes
  vos: te alarmes
  ud: se alarme
  nosotros: nos alarmemos
  vosotros: os alarméis
  uds: se alarmen

preterito-perfecto-subjuntivo:
  yo: me haya alarmado
  tu: te hayas alarmado
  vos: te hayas alarmado
  ud: se haya alarmado
  nosotros: nos hayamos alarmado
  vosotros: os hayáis alarmado
  uds: se hayan alarmado

imperfecto-subjuntivo:
  yo:
    - me alarmara
    - me alarmase
  tu:
    - te alarmaras
    - te alarmases
  vos:
    - te alarmaras
    - te alarmases
  ud:
    - se alarmara
    - se alarmase
  nosotros:
    - nos alarmáramos
    - nos alarmásemos
  vosotros:
    - os alarmarais
    - os alarmaseis
  uds:
    - se alarmaran
    - se alarmasen

pluscuamperfecto-subjuntivo:
  yo:
    - me hubiera alarmado
    - me hubiese alarmado
  tu:
    - te hubieras alarmado
    - te hubieses alarmado
  vos:
    - te hubieras alarmado
    - te hubieses alarmado
  ud:
    - se hubiera alarmado
    - se hubiese alarmado
  nosotros:
    - nos hubiéramos alarmado
    - nos hubiésemos alarmado
  vosotros:
    - os hubierais alarmado
    - os hubieseis alarmado
  uds:
    - se hubieran alarmado
    - se hubiesen alarmado

imperativo:
  tu: alármate
  vos: alarmaos
  ud: alárme
  nosotros: alarmémonos
  vosotros: alarmaos
  uds: alármense