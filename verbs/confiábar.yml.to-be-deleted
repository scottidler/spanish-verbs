  vosotros:
    - hubierais confiado
    - hubieseis confiado
  uds:
    - hubieran confiado
    - hubiesen confiado
imperativo:
  tu: confía
  vos: confiá
  ud: confíe
  nosotros: confiemos
  vosotros: confiad
  uds: confíen
