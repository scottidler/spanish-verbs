    - hubiera confiado
    - hubiese confiado
  tu:
    - hubieras confiado
    - hubieses confiado
  vos:
    - hubieras confiado
    - hubieses confiado
  ud:
    - hubiera confiado
    - hubiese confiado
  nosotros:
