    - contara
    - contase
  tu:
    - contaras
    - contases
  vos:
    - contaras
    - contases
  ud:
    - contara
    - contase
  nosotros:
    - contáramos
    - contásemos
  vosotros:
    - contarais
    - contaseis
  uds:
    - contaran
    - contasen
pluscuamperfecto-subjuntivo:
  yo:
    - hubiera contado
    - hubiese contado
  tu:
    - hubieras contado
    - hubieses contado
  vos:
    - hubieras contado
    - hubieses contado
  ud:
    - hubiera contado
    - hubiese contado
  nosotros:
    - hubiéramos contado
    - hubiésemos contado
  vosotros:
    - hubierais contado
