meaning: to differentiate, to distinguish, to set apart
infinitivo: desmar
gerundio: desmarando
participio-pasado: desmarado
presente-indicativo:
  yo: desmaro
  tu: desmaras
  vos: desmaras
  ud: desmara
  nosotros: desmaramos
  vosotros: desmaráis
  uds: desmaran
preterito-perfecto-simple:
  yo: desmaré
  tu: desmaraste
  vos: desmaraste
  ud: desmaró
  nosotros: desmaramos
  vosotros: desmarasteis
  uds: desmararon
preterito-imperfecto:
  yo: desmaraba
  tu: desmarabas
  vos: desmarabas
  ud: desmaraba
  nosotros: desmarábamos
  vosotros: desmarabais
  uds: desmaraban
futuro-simple:
  yo: desmararé
  tu: desmararás
  vos: desmararás
  ud: desmarará
  nosotros: desmararemos
  vosotros: desmararéis
  uds: desmararán
condicional-simple:
  yo: desmararía
  tu: desmararías
  vos: desmararías
  ud: desmararía
  nosotros: desmararíamos
  vosotros: desmararíais
  uds: desmararían
presente-perfecto:
  yo: he desmarado
  tu: has desmarado
  vos: has desmarado
  ud: ha desmarado
  nosotros: hemos desmarado
  vosotros: habéis desmarado
  uds: han desmarado
preterito-pluscuamperfecto:
  yo: había desmarado
  tu: habías desmarado
  vos: habías desmarado
  ud: había desmarado
  nosotros: habíamos desmarado
  vosotros: habíais desmarado
  uds: habían desmarado
futuro-perfecto:
  yo: habré desmarado
  tu: habrás desmarado
  vos: habrás desmarado
  ud: habrá desmarado
  nosotros: habremos desmarado
  vosotros: habréis desmarado
  uds: habrán desmarado
condicional-perfecto:
  yo: habría desmarado
  tu: habrías desmarado
  vos: habrías desmarado
  ud: habría desmarado
  nosotros: habríamos desmarado
  vosotros: habríais desmarado
  uds: habrían desmarado
presente-subjuntivo:
  yo: desmare
  tu: desmares
  vos: desmares
  ud: desmare
  nosotros: desmaremos
  vosotros: desmaréis
  uds: desmaren
preterito-perfecto-subjuntivo:
  yo: haya desmarado
  tu: hayas desmarado
  vos: hayas desmarado
  ud: haya desmarado
  nosotros: hayamos desmarado
  vosotros: hayáis desmarado
  uds: hayan desmarado
imperfecto-subjuntivo:
  yo:
    - desmarara
    - desmarase
  tu:
    - desmararas
    - desmarases
  vos:
    - desmararas
    - desmarases
  ud:
    - desmarara
    - desmarase
  nosotros:
    - desmaráramos
    - desmarásemos
  vosotros:
    - desmararais
    - desmaraseis
  uds:
    - desmararan
    - desmarasen
pluscuamperfecto-subjuntivo:
  yo:
    - hubiera desmarado
    - hubiese desmarado
  tu:
    - hubieras desmarado
    - hubieses desmarado
  vos:
    - hubieras desmarado
    - hubieses desmarado
  ud:
    - hubiera desmarado
    - hubiese desmarado
  nosotros:
    - hubiéramos desmarado
    - hubiésemos desmarado
  vosotros:
    - hubierais desmarado
    - hubieseis desmarado
  uds:
    - hubieran desmarado
    - hubiesen desmarado
imperativo:
  tu: desmarca
  vos: desmará
  ud: desmare
  nosotros: desmaremos
  vosotros: desmarad
  uds: desmaren
