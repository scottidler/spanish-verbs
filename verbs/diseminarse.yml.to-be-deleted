meaning: to spread, to disseminate
infinitivo: diseminarse
gerundio: diseminándose
participio-pasado: diseminado

presente-indicativo:
  yo: me disemino
  tu: te diseminas
  vos: te diseminás
  ud: se disemina
  nosotros: nos diseminamos
  vosotros: os disemináis
  uds: se diseminan

preterito-perfecto-simple:
  yo: me diseminé
  tu: te diseminaste
  vos: te diseminaste
  ud: se diseminó
  nosotros: nos diseminamos
  vosotros: os diseminasteis
  uds: se diseminaron

preterito-imperfecto:
  yo: me diseminaba
  tu: te diseminabas
  vos: te diseminabas
  ud: se diseminaba
  nosotros: nos diseminábamos
  vosotros: os diseminabais
  uds: se diseminaban

futuro-simple:
  yo: me diseminaré
  tu: te diseminarás
  vos: te diseminarás
  ud: se diseminará
  nosotros: nos diseminaremos
  vosotros: os diseminaréis
  uds: se diseminarán

condicional-simple:
  yo: me diseminaría
  tu: te diseminarías
  vos: te diseminarías
  ud: se diseminaría
  nosotros: nos diseminaríamos
  vosotros: os diseminaríais
  uds: se diseminarían

presente-perfecto:
  yo: me he diseminado
  tu: te has diseminado
  vos: te has diseminado
  ud: se ha diseminado
  nosotros: nos hemos diseminado
  vosotros: os habéis diseminado
  uds: se han diseminado

preterito-pluscuamperfecto:
  yo: me había diseminado
  tu: te habías diseminado
  vos: te habías diseminado
  ud: se había diseminado
  nosotros: nos habíamos diseminado
  vosotros: os habíais diseminado
  uds: se habían diseminado

futuro-perfecto:
  yo: me habré diseminado
  tu: te habrás diseminado
  vos: te habrás diseminado
  ud: se habrá diseminado
  nosotros: nos habremos diseminado
  vosotros: os habréis diseminado
  uds: se habrán diseminado

condicional-perfecto:
  yo: me habría diseminado
  tu: te habrías diseminado
  vos: te habrías diseminado
  ud: se habría diseminado
  nosotros: nos habríamos diseminado
  vosotros: os habríais diseminado
  uds: se habrían diseminado

presente-subjuntivo:
  yo: me disemine
  tu: te disemines
  vos: te disemines
  ud: se disemine
  nosotros: nos diseminemos
  vosotros: os diseminéis
  uds: se diseminen

preterito-perfecto-subjuntivo:
  yo: me haya diseminado
  tu: te hayas diseminado
  vos: te hayas diseminado
  ud: se haya diseminado
  nosotros: nos hayamos diseminado
  vosotros: os hayáis diseminado
  uds: se hayan diseminado

imperfecto-subjuntivo:
  yo:
    - me diseminara
    - me diseminase
  tu:
    - te diseminaras
    - te diseminases
  vos:
    - te diseminaras
    - te diseminases
  ud:
    - se diseminara
    - se diseminase
  nosotros:
    - nos disemináramos
    - nos diseminásemos
  vosotros:
    - os diseminarais
    - os diseminaseis
  uds:
    - se diseminaran
    - se disminasen

pluscuamperfecto-subjuntivo:
  yo:
    - me hubiera diseminado
    - me hubiese diseminado
  tu:
    - te hubieras diseminado
    - te hubieses diseminado
  vos:
    - te hubieras diseminado
    - te hubieses diseminado
  ud:
    - se hubiera diseminado
    - se hubiese diseminado
  nosotros:
    - nos hubiéramos diseminado
    - nos hubiésemos diseminado
  vosotros:
    - os hubierais diseminado
    - os hubieseis diseminado
  uds:
    - se hubieran diseminado
    - se hubiesen diseminado

imperativo:
  tu: disemínate
  vos: disemináte
  ud: disemínese
  nosotros: diseminémonos
  vosotros: diseminaos
  uds: disemínense