meaning: to formulate, to be formulated
infinitivo: formularse
gerundio: formulándose
participio-pasado: formulado

presente-indicativo:
  yo: me formulo
  tu: te formulas
  vos: te formulas
  ud: se formula
  nosotros: nos formulamos
  vosotros: os formuláis
  uds: se formulan

preterito-perfecto-simple:
  yo: me formulé
  tu: te formulaste
  vos: te formulaste
  ud: se formuló
  nosotros: nos formulamos
  vosotros: os formulasteis
  uds: se formularon

preterito-imperfecto:
  yo: me formulaba
  tu: te formulabas
  vos: te formulabas
  ud: se formulaba
  nosotros: nos formulábamos
  vosotros: os formulabais
  uds: se formulaban

futuro-simple:
  yo: me formularé
  tu: te formularás
  vos: te formularás
  ud: se formulará
  nosotros: nos formularemos
  vosotros: os formularéis
  uds: se formularán

condicional-simple:
  yo: me formularía
  tu: te formularías
  vos: te formularías
  ud: se formularía
  nosotros: nos formularíamos
  vosotros: os formularíais
  uds: se formularían

presente-perfecto:
  yo: me he formulado
  tu: te has formulado
  vos: te has formulado
  ud: se ha formulado
  nosotros: nos hemos formulado
  vosotros: os habéis formulado
  uds: se han formulado

preterito-pluscuamperfecto:
  yo: me había formulado
  tu: te habías formulado
  vos: te habías formulado
  ud: se había formulado
  nosotros: nos habíamos formulado
  vosotros: os habíais formulado
  uds: se habían formulado

futuro-perfecto:
  yo: me habré formulado
  tu: te habrás formulado
  vos: te habrás formulado
  ud: se habrá formulado
  nosotros: nos habremos formulado
  vosotros: os habréis formulado
  uds: se habrán formulado

condicional-perfecto:
  yo: me habría formulado
  tu: te habrías formulado
  vos: te habrías formulado
  ud: se habría formulado
  nosotros: nos habríamos formulado
  vosotros: os habríais formulado
  uds: se habrían formulado

presente-subjuntivo:
  yo: me formule
  tu: te formules
  vos: te formules
  ud: se formule
  nosotros: nos formulemos
  vosotros: os formuléis
  uds: se formulen

preterito-perfecto-subjuntivo:
  yo: me haya formulado
  tu: te hayas formulado
  vos: te hayas formulado
  ud: se haya formulado
  nosotros: nos hayamos formulado
  vosotros: os hayáis formulado
  uds: se hayan formulado

imperfecto-subjuntivo:
  yo:
    - me formulara
    - me formulase
  tu:
    - te formularas
    - te formulases
  vos:
    - te formularas
    - te formulases
  ud:
    - se formulara
    - se formulase
  nosotros:
    - nos formuláramos
    - nos formulásemos
  vosotros:
    - os formularais
    - os formulaseis
  uds:
    - se formularan
    - se formulases

pluscuamperfecto-subjuntivo:
  yo:
    - me hubiera formulado
    - me hubiese formulado
  tu:
    - te hubieras formulado
    - te hubieses formulado
  vos:
    - te hubieras formulado
    - te hubieses formulado
  ud:
    - se hubiera formulado
    - se hubiese formulado
  nosotros:
    - nos hubiéramos formulado
    - nos hubiésemos formulado
  vosotros:
    - os hubierais formulado
    - os hubieseis formulado
  uds:
    - se hubieran formulado
    - se hubiesen formulado

imperativo:
  tu: fórmulate
  vos: fórmulate
  ud: formúlese
  nosotros: formulémonos
  vosotros: formulaos
  uds: formulense