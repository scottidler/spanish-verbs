meaning: to read
infinitivo: leer
gerundio: leyendo
participio-pasado: leído

presente-indicativo:
  yo: leo
  tu: lees
  vos: lees
  ud: lee
  nosotros: leemos
  vosotros: leéis
  uds: leen

