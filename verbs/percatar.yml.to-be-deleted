meaning: to perceive, to notice, to become aware
infinitivo: percatar
gerundio: percatando
participio-pasado: percatado
presente-indicativo:
  yo: percato
  tu: percatas
  vos: percatas
  ud: percata
  nosotros: percatamos
  vosotros: percatáis
  uds: percatan
preterito-perfecto-simple:
  yo: percaté
  tu: percataste
  vos: percataste
  ud: percató
  nosotros: percatamos
  vosotros: percasteis
  uds: percataron
preterito-imperfecto:
  yo: percataba
  tu: percatabas
  vos: percatabas
  ud: percataba
  nosotros: percatábamos
  vosotros: percatabais
  uds: percataban
futuro-simple:
  yo: percataré
  tu: percatarás
  vos: percatarás
  ud: percatará
  nosotros: percataremos
  vosotros: percataréis
  uds: percatarán
condicional-simple:
  yo: percataría
  tu: percatarías
  vos: percatarías
  ud: percataría
  nosotros: percataríamos
  vosotros: percataríais
  uds: percatarían
presente-perfecto:
  yo: he percatado
  tu: has percatado
  vos: has percatado
  ud: ha percatado
  nosotros: hemos percatado
  vosotros: habéis percatado
  uds: han percatado
