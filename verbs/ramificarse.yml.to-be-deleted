meaning: to branch out
infinitivo: ramificarse
gerundio: ramificándose
participio-pasado: ramificado

presente-indicativo:
  yo: me ramifico
  tu: te ramificas
  vos: te ramificas
  ud: se ramifica
  nosotros: nos ramificamos
  vosotros: os ramificáis
  uds: se ramifican

preterito-perfecto-simple:
  yo: me ramifiqué
  tu: te ramificaste
  vos: te ramificaste
  ud: se ramificó
  nosotros: nos ramificamos
  vosotros: os ramificasteis
  uds: se ramificaron

preterito-imperfecto:
  yo: me ramificaba
  tu: te ramificabas
  vos: te ramificabas
  ud: se ramificaba
  nosotros: nos ramificábamos
  vosotros: os ramificabais
  uds: se ramificaban

futuro-simple:
  yo: me ramificaré
  tu: te ramificarás
  vos: te ramificarás
  ud: se ramificará
  nosotros: nos ramificaremos
  vosotros: os ramificaréis
  uds: se ramificarán

condicional-simple:
  yo: me ramificaría
  tu: te ramificarías
  vos: te ramificarías
  ud: se ramificaría
  nosotros: nos ramificaríamos
  vosotros: os ramificaríais
  uds: se ramificarían

presente-perfecto:
  yo: me he ramificado
  tu: te has ramificado
  vos: te has ramificado
  ud: se ha ramificado
  nosotros: nos hemos ramificado
  vosotros: os habéis ramificado
  uds: se han ramificado

preterito-pluscuamperfecto:
  yo: me había ramificado
  tu: te habías ramificado
  vos: te habías ramificado
  ud: se había ramificado
  nosotros: nos habíamos ramificado
  vosotros: os habíais ramificado
  uds: se habían ramificado

futuro-perfecto:
  yo: me habré ramificado
  tu: te habrás ramificado
  vos: te habrás ramificado
  ud: se habrá ramificado
  nosotros: nos habremos ramificado
  vosotros: os habréis ramificado
  uds: se habrán ramificado

condicional-perfecto:
  yo: me habría ramificado
  tu: te habrías ramificado
  vos: te habrías ramificado
  ud: se habría ramificado
  nosotros: nos habríamos ramificado
  vosotros: os habríais ramificado
  uds: se habrían ramificado

presente-subjuntivo:
  yo: me ramifique
  tu: te ramifiques
  vos: te ramifiques
  ud: se ramifique
  nosotros: nos ramifiquemos
  vosotros: os ramifiquéis
  uds: se ramifiquen

preterito-perfecto-subjuntivo:
  yo: me haya ramificado
  tu: te hayas ramificado
  vos: te hayas ramificado
  ud: se haya ramificado
  nosotros: nos hayamos ramificado
  vosotros: os hayáis ramificado
  uds: se hayan ramificado

imperfecto-subjuntivo:
  yo:
    - me ramificara
    - me ramificase
  tu:
    - te ramificaras
    - te ramificases
  vos:
    - te ramificaras
    - te ramificases
  ud:
    - se ramificara
    - se ramificase
  nosotros:
    - nos ramificáramos
    - nos ramificásemos
  vosotros:
    - os ramificarais
    - os ramificaseis
  uds:
    - se ramificaran
    - se ramificasen

pluscuamperfecto-subjuntivo:
  yo:
    - me hubiera ramificado
    - me hubiese ramificado
  tu:
    - te hubieras ramificado
    - te hubieses ramificado
  vos:
    - te hubieras ramificado
    - te hubieses ramificado
  ud:
    - se hubiera ramificado
    - se hubiese ramificado
  nosotros:
    - nos hubiéramos ramificado
    - nos hubiésemos ramificado
  vosotros:
    - os hubierais ramificado
    - os hubieseis ramificado
  uds:
    - se hubieran ramificado
    - se hubiesen ramificado

imperativo:
  tu: ramifícate
  vos: ramificate
  ud: ramifíquese
  nosotros: ramifiquémonos
  vosotros: ramificaos
  uds: ramifíquense